{
  "apps": [
    {
      "name": "99AI-Plugin",
      "script": "./dist/main.js",
      "instances": 1,
      "watch": true,
      "ignore_watch": ["node_modules", "logs"],
      "env": {
        "TZ": "Asia/Shanghai"
      },
      "error_file": "logs/err.log",
      "out_file": "logs/out.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "max_memory_restart": "1000M"
    }
  ]
}
